{
  "name": "leveldb.org",
  "version": "2.0.0",
  "description": "This is the source code for [leveldb.org](http://leveldb.org).",
  "devDependencies": {
    "jade": "^1.11.0",
    "stylus": "^0.42.3"
  },
  "scripts": {
    "build": "npm run build:templates & npm run build:syles",
    "watch": "npm run watch:templates & npm run watch:styles",
    "build:templates": "jade ./templates/index.jade -o ./assests/index.html",
    "watch:templates": "jade -w ./templates/index.jade -o ./assests/index.html",
    "build:styles": "stylus ./styles/index.styl -o ./assests/css/index.css",
    "watch:styles": "stylus -w ./styles/index.styl -o ./assests/css/index.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Level/leveldb.org.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Level/leveldb.org/issues"
  },
  "homepage": "https://github.com/Level/leveldb.org#readme"
}
